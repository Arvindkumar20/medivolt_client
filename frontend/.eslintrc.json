{
  "extends": ["@eslint/js/recommended", "@vitejs/eslint-config-react"],
  "rules": {
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_",
        "caughtErrorsIgnorePattern": "^_"
      }
    ],
    "react/prop-types": "off",
    "react/no-unescaped-entities": "off"
  },
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  }
}
